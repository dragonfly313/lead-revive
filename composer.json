{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories":
    [
        {
            "type": "vcs",
            "url": "https://github.com/gfernandez-me/Beautymail/"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "akaunting/laravel-language": "^1.0",
        "binarytorch/larecipe": "^2.4",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "^2.15",
        "guzzlehttp/guzzle": "^7.2",
        "hisorange/browser-detect": "^4.4",
        "jamesmills/laravel-timezone": "^1.9",
        "jantinnerezo/livewire-alert": "^2.2",
        "joedixon/laravel-translation": "^1.1",
        "joelbutcher/socialstream": "^1.1",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/cashier": "^12.4",
        "laravel/framework": "^9.0",
        "laravel/jetstream": "^2.3",
        "laravel/sanctum": "^2.14.1",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.5",
        "laravelcollective/html": "^6.2",
        "laraveldaily/laravel-charts": "^0.1.29",
        "league/csv": "^9.15",
        "lesichkovm/laravel-advanced-model": "^1.9",
        "lesichkovm/laravel-advanced-route": "^1.9",
        "livewire/livewire": "^2.10",
        "mckenziearts/laravel-notify": "^2.4",
        "mews/purifier": "^3.3",
        "rappasoft/laravel-livewire-tables": "^1.11.0",
        "rappasoft/lockout": "^4.0",
        "snowfire/beautymail": "dev-master",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-permission": "^5.5",
        "square/square": "^37.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.8",
        "laravel/pint": "^1.0",
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.3.3",
        "imanghafoori/laravel-microscope": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "treeware/plant": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
